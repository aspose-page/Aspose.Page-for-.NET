
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>XPS, PS & EPS File Format Solution </h1>
				<h4>Intergrate On-Premise APIs to create, manipulate or convert XPS, EPS, and PS files from within your own .NET & Java applications. Or use our Free Apps to view or convert files on the fly. </h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_page-brand.png" alt="Aspose.Page" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposePAGE"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_viewer-app.png" alt="Aspose.Page Product Family" />
					</div>
					<a href="/page/viewer"><span class="spanclass">@Model.Resources["AsposePAGE"] @Model.Resources["AsposeProductFor"]  <em>@Model.Resources["Viewerh4"] </em></span><em>Free App to view XPS, EPS and PS file formats.</em></a>
				</div>

			</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_conversion-app.png" alt="Aspose.Page for .NET" />
					</div>
					<a href="/page/conversion"><span class="spanclass">@Model.Resources["AsposePAGE"] @Model.Resources["AsposeProductFor"]  <em>Conversion</em></span><em>Convert XPS, PS and EPS files to PDF and raster image formats.</em></a>
				</div>
			</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_signature-app.png" alt="Aspose.Page for .NET" />
					</div>
					<a href="/page/signature"><span class="spanclass">@Model.Resources["AsposePAGE"] @Model.Resources["AsposeProductFor"]  <em>Signature</em></span><em>Sign XPS douments with Text Image or Drawing using any of your favourite browser.</em></a>
				</div>
			</div>

		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>